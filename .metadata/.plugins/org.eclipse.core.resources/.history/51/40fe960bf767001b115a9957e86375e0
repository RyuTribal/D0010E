package CustomerSimulator.controller;

import CustomerSimulator.models.Customer;
import CustomerSimulator.models.Store;

public class Pick extends Event {

	private Store store;
	private Customer customer;
	private EventQueue events;
	public Pick(Store s, EventQueue q, Customer c) {
		this.store = s;
		this.events = q;
		this.customer = c;
	}
	public void run() {
		System.out.println("Customer "+customer.getID()+" picked at: "+this.store.getNextPickTime());
		
	}

}
