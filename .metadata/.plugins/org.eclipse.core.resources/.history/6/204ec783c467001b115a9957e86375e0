package labb5.model;

import java.util.Observable;

import labb5.controller.Event;
import labb5.view.View;

public class State extends Observable {
	public boolean simRunning;
	
	private double timeRan;
	private View view;
	
	public State(){
		view = new View();
		addObserver(view);
		simRunning = true;
	}
	
	public void stopSim() {
		simRunning = false;
	}
	
}