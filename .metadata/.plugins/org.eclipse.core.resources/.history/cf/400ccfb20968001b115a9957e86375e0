package CustomerSimulator.controller;

import CustomerSimulator.models.Store;

public class StoreClose extends Event {
	private Store store;
	private double time;
	public StoreClose(Store s, double t) {
		this.store = s;
		this.time = t;
	}

	public void run() {
		this.store.closeStore();
	}
	public double getTime() {
		return time;
	}
}
