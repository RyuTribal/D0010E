package lab4.data;

public class GomokuGrid {
    public static final int EMPTY = 0;
    public static final int ME = 1;
    public static final int OTHER = 2;
    public static final int INROW = 5;
	
	public int[][] grid;
	
	GomokuGrid(int size){
		grid = new int[size][size];
	}
	
	public boolean isWinner() {
		return 5 == INROW;
	}
}
