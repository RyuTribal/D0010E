package CustomerSimulator.controller;

import CustomerSimulator.models.Customer;
import CustomerSimulator.models.Store;

public class Arrive extends Event {
	private Store store;
	private Customer customer;
	private EventQueue events;
	public Arrive(Store s, EventQueue q) {
		this.store = s;
		this.events = q;
	}
	public void run() {
		customer = new Customer(this.store.getNewCustomerId(),this.store);
		System.out.println("NIGGERS?!");
		events.addEvent(new Pick(this.store, this.events));
	}

}
